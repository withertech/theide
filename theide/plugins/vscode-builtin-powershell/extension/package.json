{
  "name": "powershell",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.39.1-prel",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "*"
  },
  "contributes": {
    "languages": [
      {
        "id": "powershell",
        "extensions": [
          ".ps1",
          ".psm1",
          ".psd1",
          ".pssc",
          ".psrc"
        ],
        "aliases": [
          "PowerShell",
          "powershell",
          "ps",
          "ps1"
        ],
        "firstLine": "^#!\\s*/.*\\bpwsh\\b",
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "powershell",
        "scopeName": "source.powershell",
        "path": "./syntaxes/powershell.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "powershell",
        "path": "./snippets/powershell.json"
      }
    ]
  },
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/theia-ide/vscode-builtin-extensions"
  }
}