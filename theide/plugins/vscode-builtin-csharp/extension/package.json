{
  "name": "csharp",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.39.1-prel",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "0.10.x"
  },
  "scripts": {},
  "contributes": {
    "languages": [
      {
        "id": "csharp",
        "extensions": [
          ".cs",
          ".csx",
          ".cake"
        ],
        "aliases": [
          "C#",
          "csharp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "csharp",
        "scopeName": "source.cs",
        "path": "./syntaxes/csharp.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "csharp",
        "path": "./snippets/csharp.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theia-ide/vscode-builtin-extensions"
  }
}