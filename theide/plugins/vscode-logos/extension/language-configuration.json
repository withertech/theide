{
	"comments": {
		"lineComment": "//",
		"blockComment": ["/*", "*/"]
	},
	"brackets": [
		["%hook", "%end"],
		["%hookf", "%end"],
		["%group", "end"],

		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["%hook", "%end"],
		["%hookf", "%end"],
		["%group", "end"],

		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"]
	],
	"surroundingPairs": [
		["%hook", "%end"],
		["%hookf", "%end"],
		["%group", "end"],

		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"]
	],
	"folding": {
		"markers": {
			"start": "%hook|%hookf|%group*(.*)",
			"end": "%end"
		}
	}
}
