{
	"name": "logos-vscode",
	"displayName": "Logos",
	"description": "Logos Syntax Support for Visual Studio Code",
	"version": "0.5.1",
	"publisher": "Renaitare",
	"icon": "assets/logos-vscode.png",
	"engines": {
		"vscode": "^1.46.0"
	},
	"author": {
		"name": "Renaitare",
		"email": "support@renai.me",
		"url": "https://renai.me"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Renaitare/Logos.git"
	},
	"galleryBanner": {
		"color": "#005577",
		"theme": "dark"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"objective-c",
		"objective-c++",
		"logos",
		"vscode"
	],
	"activationEvents": [
		"onLanguage:logos"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "logos",
				"aliases": [
					"Logos",
					"logos"
				],
				"extensions": [
					".x",
					".xm",
					".xi",
					".xmi"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "logos",
				"scopeName": "source.logos",
				"path": "./syntaxes/logos.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^14.0.19",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"prettier": "^2.0.5",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
